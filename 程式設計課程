#include<stdio.h> 
#include<stdlib.h>
int main(){
int x = 10,y = 100,tmp ;
tmp=y;
y=x;
x=tmp;
printf("%d,%d\n",x,y);
## 100,10

x=x^y;
y=x^y;
x=x^y;
printf("%d,%d\n",x,y);
system("pause");
	return 0;
}
