#include<stdio.h> 
#include<stdlib.h>
int main(){
char ch='6';//0011 0110
char mir=127;//0111 1111
printf("%c:%c\n",ch,(~ch)&mir);//~ch=1100 1001
                    //(~ch)&mir =0100 1001
	system("PAUSE");

	return 0;
}
##6:I
